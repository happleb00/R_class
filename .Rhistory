complete <- function(directory, id = 1:332) {
files_list <- list.files(directory, full.names = TRUE)
dat <- data.frame()
for (i in id) {
dat <- rbind(dat, read.csv(files_list[i]))
}
list.files(dat[,pollutant])
}
complete("specdata", 1)
